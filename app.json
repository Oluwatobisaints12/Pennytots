{
  "expo": {
    "name": "PennytotExpo",
    "slug": "pennytots",
    "version": "1.0.0",
    "updates": {
      "useClassicUpdates": true
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PennytotExpo to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow PennytotExpo to access your photos",
          "cameraPermission": "Allow PennytotExpo to access your camera"
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow PennytotExpo to access your contacts."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "app/assets/logo_new.png",
          "color": "#ffffff"
          
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "Allow PennytotExpo to access your contacts."
      },
      "bundleIdentifier": "com.ibkay998.pennytots"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "861158c3-27ca-411b-b2ff-6ff99afceb62"
      }
    },
    "owner": "ibkay998"
  }
}
